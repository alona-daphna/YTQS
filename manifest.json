{
    "name": "YouTube Quick Search",
    "short_name": "YTQS",
    "version": "1.0.0",
    "description": "Search YouTube fast & without getting sidetracked",
    "manifest_version": 3,
    "author": "Alona Daphna",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_title": "YouTube Quick Search"
    },
    "permissions": [
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    }
}